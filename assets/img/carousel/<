site mdah
  server ftp.lautre.net
  username espierre
  password La2Bro12
  local /home/in/MyDomoAtHome/binary
  remote /e-nef/domoticz/mdah
  state checksum
  checkmoved renames
  exclude *.log
  exclude node-mydomoathome-1.deb

site r2
