  exclude node-mydomoathome-1.deb

site r2ovh
  server ftp.cluster023.hosting.ovh.net
  username romanesczd
  password J6Mqn8eqz6Aq
  local /mnt/data/prod/romanes2.com
  remote /www/
  state checksum
  checkmoved renames
  symlink follow
  exclude *.log
  exclude $.deb
  exclude $.zip
  exclude /.git
  exclude /01

site r3ovh
